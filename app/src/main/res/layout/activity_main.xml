        android:text="" />


        -->

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ID IC Card Test"
            android:id="@+id/btnIDICCard_Test"

            android:onClick="ID_ICCard_Test_click" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >
        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Finger Printer Test"
            android:id="@+id/btnFingerPrinter_Test"

            android:onClick="FingerPrinter_Test_click" />
    </LinearLayout>
</LinearLayout>
